{
  "name": "fjl-validator-recaptcha",
  "version": "1.2.2",
  "main": "./dist/cjs/reCaptchaValidator.js",
  "module": "./src/reCaptchaValidator.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "pre-publish": "yarn build && yarn docs",
    "build": "gulp clean && gulp build && yarn test-builds",
    "docs": "gulp jsdoc",
    "start": "forever start tests/mock-server.js",
    "stop": "forever stop tests/mock-server.js",
    "pretest": "yarn start",
    "test": "jest --testMatch **/tests/test-*.js",
    "test-builds": "jest --testMatch **/tests/*-test-*.js",
    "posttest": "yarn stop",
    "eslint": "eslint"
  },
  "dependencies": {
    "fjl": "1.6.2",
    "fjl-mutable": "^0.18.3",
    "fjl-validator": "^0.7.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "forever": "^0.15.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-eslint": "^4.0.2",
    "gulp-jsdoc3": "^1.0.1",
    "helmet": "^3.9.0",
    "jest": "^22.1.4",
    "lazypipe": "^1.0.1",
    "puppeteer": "^1.0.0"
  },
  "recaptchaKeys": {
    "siteKey": "6LeIxAcTAAAAAJcZVRqyHh71UMIEGNQ_MXjiZKhI",
    "secretKey": "6LeIxAcTAAAAAGG-vFI1TnRWxMZNFuojJ4WifJWe"
  },
  "mockServerPort": 10087,
  "jest": {
    "testMatch": [
      "**/tests/test-*.js"
    ],
    "testEnvironment": "node"
  },
  "buildConfig": {
    "inputModuleName": "reCaptchaValidator",
    "inputFilePath": "./src/reCaptchaValidator.js",
    "outputFileName": "fjl-validator-recaptcha.js",
    "srcsGlob": "./src/**/*.js",
    "paths": {
      "dist": "./dist",
      "docs": "./docs"
    },
    "folderNames": {
      "cjs": "cjs"
    },
    "cjsRollup": {
      "babelPlugin": {
        "babelrc": false,
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ]
        ],
        "plugins": [
          "babel-plugin-add-module-exports"
        ]
      }
    }
  },
  "repository": "git@github.com:functional-jslib/fjl-validator-recaptcha.git",
  "author": "Ely De La Cruz <elycruz@elycruz.com>"
}
